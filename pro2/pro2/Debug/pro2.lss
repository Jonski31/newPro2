                                 .macro incrementCoins
                                 	lds temp, coins
                                 	inc temp
                                 	sts coins, temp
                                 	clr temp
                                 .endMacro
                                 
                                 .macro decrementCoins
                                 	lds temp, coins
                                 	dec temp
                                 	sts coins, temp
                                 	clr temp
                                 .endMacro
                                 
                                 
                                 
