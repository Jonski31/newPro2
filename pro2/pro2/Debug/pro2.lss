                                 // STACK MACROS //////////////////////
                                 .macro pushStack
                                 	push temp
                                 	push temp1
                                 	push temp2
                                 	in temp, SREG
                                 	push temp ; Prologue starts.
                                 	push YH ; Save all conflict registers in the prologue.
                                 	push YL
                                 	push r25
                                 	push r24 ; Prologue ends.
                                 .endmacro
                                 
                                 
                                 .macro popStack
                                 	pop r24 ; Epilogue starts;
                                 	pop r25 ; Restore all conflict registers from the stack.
                                 	pop YL
                                 	pop YH
                                 	pop temp
                                 	out SREG, temp
                                 	pop temp2
                                 	pop temp1
                                 	pop temp
                                 .endmacro
